generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Token {
   userId      String @id @default("default_user")
  accessToken String
}

model ScheduledMessage {
  // ... this model does not need to change
  id        String @id
  channelId String
  postAt    Int
  text      String
}